# Run tests for skylab_suite.
set -eu
readonly bin_dir="$(readlink -e -- "$(dirname -- "$0")")"

# cd for pytest configuration files
cd "$bin_dir/../venv"

"${bin_dir}/python_venv" -m pytest skylab_staging "$@"
