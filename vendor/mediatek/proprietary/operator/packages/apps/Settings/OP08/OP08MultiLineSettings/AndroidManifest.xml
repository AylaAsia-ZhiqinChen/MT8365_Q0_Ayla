<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mediatek.op08.settings.mulitine"
          coreApp="true"
          android:versionCode="1"
          android:versionName="1.0"
          >
          <!-- Do not use "android.uid.system" as sharedUserId, beacuse plugin is using WebView,
                    which do render in system process -->

    <uses-permission android:name="android.permission.WRITE_SECURE_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.DIGITS" />

    <uses-sdk android:minSdkVersion="24"/>

   

    <!-- Add your plug-in service here -->
    <application android:label="@string/multi_line">

        <activity
            android:name=".MultiLineSettingsActivity"
            android:theme="@android:style/Theme.Material.Settings"
            android:label="@string/multi_line"
            android:configChanges="orientation|screenSize|keyboardHidden|mcc|mnc"
            android:process="com.android.phone">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.mediatek.op08.settings.mulitine.MultiLineActivity"
                android:icon="@drawable/ic_call_settings"
                android:label="@string/multi_line"
                android:configChanges="orientation|screenSize|keyboardHidden|mcc|mnc"
                android:launchMode="singleTask"
                android:clearTaskOnLaunch="true"
                android:noHistory="true">
            <intent-filter android:priority="0">
                <action android:name="com.android.settings.action.EXTRA_SETTINGS" />
            </intent-filter>
            <meta-data android:name="com.android.settings.category"
                android:value="com.android.settings.category.ia.wireless" />
            <meta-data android:name="com.android.settings.icon"
                android:resource="@drawable/ic_call_settings" />
        </activity>
        <activity android:name=".ActivateNew"></activity>
        <activity android:name=".LoginActivity"></activity>
    </application>

</manifest>
