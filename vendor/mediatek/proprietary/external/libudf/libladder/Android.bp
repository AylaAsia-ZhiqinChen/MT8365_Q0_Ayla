cc_library_headers {
    name: "libladder_headers",
    export_include_dirs: [
        "",
        ]
}
cc_library {
    srcs: ["libladder.cpp"],
    name: "libladder",
    proprietary: true,
    owner: "mtk",
    include_dirs: [
        "system/core/libunwindstack",
        "system/core/base/include",
        ],
    shared_libs: [
        "libunwindstack",
        "liblog",
        "libbase",
     ],
    allow_undefined_symbols: true,
     cflags: [
	"-Wexit-time-destructors",
	"-DNO_LIBDEXFILE_SUPPORT",
    ],

}
