<merge xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- U+0911: "ऑ" BENGALI LETTER CANDRA O -->
            <Key latin:keySpec="&#x0B5C;" />
            <!-- Because the font rendering system prior to API version 16 can't automatically
                 render dotted circle for incomplete combining letter of some scripts, different
                 set of Key definitions are needed based on the API version. -->
            <include latin:keyboardLayout="@xml/keystyle_oriya_sign_candrabindu" />
            <Key latin:keyStyle="baseKeyOriyaSignCandrabindu" />
            <!-- U+0923: "ण" BENGALI LETTER NNA -->
            <Key latin:keySpec="&#x0B23;" />
            <!-- U+0929: "ऩ" BENGALI LETTER NNNA -->
            <Key latin:keySpec="&#x0B5D;" />
            <!-- U+0933: "ळ" BENGALI LETTER LLA
                 U+0934: "ऴ" BENGALI LETTER LLLA -->
            <Key
                latin:keySpec="&#x0B33;" />
            <!-- U+0936: "श" BENGALI LETTER SHA -->
            <Key latin:keySpec="&#x0B36;" />
            <!-- U+0937: "ष" BENGALI LETTER SSA -->
            <Key latin:keySpec="&#x0B37;" />
            <!-- Because the font rendering system prior to API version 16 can't automatically
                 render dotted circle for incomplete combining letter of some scripts, different
                 set of Key definitions are needed based on the API version. -->
            <include latin:keyboardLayout="@xml/keystyle_oriya_vowel_sign_vocalic_r" />
            <Key latin:keyStyle="baseKeyOriyaVowelSignVocalicR" />
            <!-- U+091E: "ञ" BENGALI LETTER NYA -->
            <Key latin:keySpec="&#x0B1E;" />
        </case>
        <default>
            <!-- Because the font rendering system prior to API version 16 can't automatically
                 render dotted circle for incomplete combining letter of some scripts, different
                 set of Key definitions are needed based on the API version. -->
            <include latin:keyboardLayout="@xml/keystyle_oriya_vowel_sign_candra_o" />
            <Key latin:keyStyle="baseKeyOriyaVowelSignCandraO" />
            <!-- Because the font rendering system prior to API version 16 can't automatically
                 render dotted circle for incomplete combining letter of some scripts, different
                 set of Key definitions are needed based on the API version. -->
            <include latin:keyboardLayout="@xml/keystyle_oriya_sign_anusvara" />
            <Key latin:keyStyle="baseKeyOriyaSignAnusvara" />
            <!-- U+092E: "म" BENGALI LETTER MA
                 U+0950: "ॐ" BENGALI OM -->
            <Key
                latin:keySpec="&#x0B2E;"
                latin:moreKeys="&#x0B70;,&#x0B63;,&#x0B35;" />
            <!-- U+0928: "न" BENGALI LETTER NA
                 U+091E: "ञ" BENGALI LETTER NYA
                 U+0919: "ङ" BENGALI LETTER NGA
                 U+0928/U+093C: "ऩ" BENGALI LETTER NA/BENGALI SIGN NUKTA -->
            <Key
                latin:keySpec="&#x0B28;"
                latin:moreKeys="&#x0B1E;,&#x0B19;,&#x0B28;&#x0B3C;" />
            <!-- U+0935: "व" BENGALI LETTER VA -->
            <Key latin:keySpec="&#x0B30;" />
            <!-- U+0932: "ल" BENGALI LETTER LA
                 U+090C: "ऌ" BENGALI LETTER VOCALIC L
                 U+0961: "ॡ" BENGALI LETTER VOCALIC LL -->
            <Key
                latin:keySpec="&#x0B32;"
                latin:moreKeys="&#x0B0C;,&#x0B61;" />
            <!-- U+0938: "स" BENGALI LETTER SA -->
            <Key latin:keySpec="&#x0B38;"
            	   latin:moreKeys="&#x0965;" />
            <!-- U+092F: "य" BENGALI LETTER YA
                 U+095F: "य़" BENGALI LETTER YYA -->
            <Key
                latin:keySpec="&#x0B2F;"
                latin:moreKeys="&#x0B5F;" />
            <!-- Because the font rendering system prior to API version 16 can't automatically
                 render dotted circle for incomplete combining letter of some scripts, different
                 set of Key definitions are needed based on the API version. -->
            <include latin:keyboardLayout="@xml/keystyle_oriya_sign_nukta" />
            <Key latin:keyStyle="baseKeyOriyaSignNukta" />
        </default>
    </switch>
</merge>
