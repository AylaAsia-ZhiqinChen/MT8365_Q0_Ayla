<?xml version="1.0" encoding="utf-8"?>

<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keySpec="&#x106b;" />
            <Key latin:keySpec="&#x106c;" latin:moreKeys="&#x1097;"/>
            <Key latin:keySpec="&#x106d;" latin:moreKeys="&#x1092;"/>
            <Key latin:keySpec="&#x106e;" />
            <Key latin:keySpec="&#x106f;" />
            <Key latin:keySpec="&#x1070;" latin:moreKeys="&#x1091;" />
            <Key latin:keySpec="&#x1071;" latin:moreKeys="&#x1072;"/>
            <Key latin:keySpec="&#x1073;" latin:moreKeys="&#x1074;"/>
            <Key latin:keySpec="&#x1075;" />
            <Key latin:keySpec="&#x1076;" />
        </case>
        <default>
            <Key latin:keySpec="&#x100a;"/>
            <Key latin:keySpec="&#x100b;"/>
            <Key latin:keySpec="&#x100c;"/>
            <Key latin:keySpec="&#x100d;"/>
            <Key latin:keySpec="&#x100e;"/>
            <Key latin:keySpec="&#x100f;"/>
            <Key latin:keySpec="&#x1010;"/>
            <Key latin:keySpec="&#x1011;"/>
            <Key latin:keySpec="&#x1012;"/>
            <Key latin:keySpec="&#x1013;"/>
        </default>
    </switch>
</merge>
