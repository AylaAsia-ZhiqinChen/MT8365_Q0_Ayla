<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

     <PreferenceCategory
        android:key="category_alert_settings"
        android:title="@string/emergency_alert_settings_title" >
        <CheckBoxPreference
            android:defaultValue="true"
            android:enabled="false"
            android:key="enable_presidential_level_alert"
            android:title="@string/pws_cmas_presidential_level_alert_chinese"
            android:summary="@string/pws_cmas_presidential_level_alert_english"/>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="enable_message_identifiers"
            android:title="@string/pws_other_message_identifiers_chinese"
            android:summary="@string/pws_other_message_identifiers_english"/>
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="enable_cmas_alerts_messages"
            android:title="@string/cmas_alert_message_chinese"
            android:summary="@string/cmas_alert_message_english"/>
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="enable_cmas_rmt_support"
            android:title="@string/cmas_required_monthly_test_chinese"
            android:summary="@string/cmas_required_monthly_test_english"/>
     </PreferenceCategory>
</PreferenceScreen>