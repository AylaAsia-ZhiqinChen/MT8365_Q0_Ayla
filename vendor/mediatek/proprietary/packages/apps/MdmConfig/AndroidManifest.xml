<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mediatek.mdmconfig"
    android:sharedUserId="android.uid.system">

    <permission
        android:name="com.mediatek.mdmconfig.START"
        android:label="Start MDMConfig"
        android:protectionLevel="signature" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MdmConfig"
            android:configChanges="orientation"
            android:label="@string/activity_title_mdm_start"
            android:screenOrientation="portrait"
            android:permission="com.mediatek.mdmconfig.START" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>
