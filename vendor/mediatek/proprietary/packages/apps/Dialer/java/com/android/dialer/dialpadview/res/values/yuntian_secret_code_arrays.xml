<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--	暗码指令参数配置	-->
    <string-array name="secret_code_parameter">

        <!--
            隐式启动activity.
            参数说明:以";"分隔参数
            第一个参数为指令,如:*#1234#,可以修改成*#*#1111#*#*,或*#111#,或#1111#,也可修改成**1111##等等;
            第二个参数为action,即android代码里面的intent.setAction("android.intent.action.yuntiansecretcode");
            第三个参数为extra,即android代码里面的intent.setExtra("type",1);
        -->
        <!--   显示Software Version   -->
        <item>*#0901#;yuntiansecretcode;extra:type=1</item>
        <!--   显示Git Info   -->
        <item>*#0902#;yuntiansecretcode;extra:type=2</item>
        <!--   显示Hareware Version   -->
        <item>*#0903#;yuntiansecretcode;extra:type=3</item>
        <!--   显示Real Rom   -->
        <item>*#0904#;yuntiansecretcode;extra:type=4</item>
        <!--   显示Battery Info   -->
        <item>*#0905#;yuntiansecretcode;extra:type=5</item>
        <!--
            显式启动activity.
            参数说明:以";"分隔参数
            第一个参数为指令,如:*#1234#,可以修改成*#*#1111#*#*,或*#111#,或#1111#,也可修改成**1111##等等;
            第二个参数为包类名,以"/"分割开，前面为包名，后面为类名
        -->
        <!--   进入工厂测试模式   -->
        <item>*#1001#;com.android.factorytest/com.android.factorytest.FactoryTest</item>
        <!--   进入工程模式   -->
        <item>*#1002#;com.mediatek.engineermode/com.mediatek.engineermode.EngineerMode</item>

        <!--   切换动画   -->
        <item>*#1101#;com.android.switchbootanimation/com.android.switchbootanimation.SwitchBootAnimationActivity</item>
        <!--   写Imei号   -->
        <item>*#1102#;com.android.imeisettings/com.android.imeisettings.ImeiSettings</item>
        <!--   进入老化测试、压力测试   -->
        <item>*#1103#;com.android.agingtest/com.android.agingtest.AgingTestMainActivity</item>       
        <!--   进入AT指令测试  -->
        <item>*#1201#;attest.yuntian.com.attest/attest.yuntian.com.attest.AtTestActivity</item> 
         <!--  天通模式选项  -->
        <item>*#1202#;com.y.modeselect/com.y.modeselect.MainActivity</item>  
        <!--   添加指令，只需要按照上面的规则添加一个item即可   -->

    </string-array>

</resources>
