<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Simme lock request start -->
    <!-- Feature configuration to show simme lock screen -->
    <!-- 0 means disabled and 1 for enabled -->
    <string name="simme_dependency_enabled">1</string>
    <!-- whether to show lock screen -->
    <!-- Use bitmask for special slot id. 0001 means slot0 and 0010 for slot1 -->
    <string name="simme_lock_with_request_4">1</string>
    <string name="simme_lock_with_request_5">2</string>

    <!-- SubsidyLock -->
    <string name="simme_lock_with_request_10">0</string>

    <!-- Parse value from config is 17,enable request 8 -->
    <string name="simme_lock_with_request_8">17</string>

    <!-- Feature configuration to show dismiss button -->
    <!-- 0 means disabled and 1 for enabled -->
    <string name="dismiss_button_enabled">1</string>
    <!-- whether to show dismiss button -->
    <string name="dismiss_button_with_request_255">0</string>
    <!-- Simme lock request end -->

</resources>
