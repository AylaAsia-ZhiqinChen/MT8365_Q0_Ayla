cc_library_static {
    name: "libtflite_mtk_np_ext",
    defaults: ["tflite_mtk_defaults"],
    srcs: [
        "mtk_helper.cc",
        "mtk_interpreter.cc",
        "core/mtk_subgraph.cc",
        "delegates/nnapi/mtk_nnapi_delegate.cc",
        "delegates/nnapi/mtk_nnapi_delegate_p.cc",
        "delegates/nnapi/mtk_nnapi_delegate_q.cc",
        "kernels/mtk_abs.cc",
        "kernels/mtk_axis_aligned_bbox_transform.cc",
        "kernels/mtk_box_with_nms_limit.cc",
        "kernels/mtk_channel_shuffle.cc",
        "kernels/mtk_crop_and_resize.cc",
        "kernels/mtk_depth_to_space.cc",
        "kernels/mtk_elu.cc",
        "kernels/mtk_generate_proposals.cc",
        "kernels/mtk_layer_normalization.cc",
        "kernels/mtk_opt.cc",
        "kernels/mtk_quantize.cc",
        "kernels/mtk_register.cc",
        "kernels/mtk_requantize.cc",
        "kernels/mtk_reverse.cc",
        "kernels/mtk_roi_align.cc",
        "kernels/mtk_transpose_conv.cc",
    ],
    shared_libs: [
        "libnativewindow",
    ],
    whole_static_libs: [
        "libarect",
    ],
    header_libs: [
        "flatbuffer_headers",
        "libeigen",
        "gemmlowp_mtk_headers",
    ],
    cflags: [
        "-Wno-array-bounds",
        "-Wno-extern-c-compat",
        "-Wno-missing-field-initializers",
        "-Wno-invalid-partial-specialization",
        "-Wno-unused-local-typedef",
        "-Wno-unused-private-field",
        "-Wno-unused-variable",
        "-Wno-sign-compare",
        "-Wno-varargs",
    ],
}

cc_library_static {
    name: "libtflite_mtk_np_ext_utils",
    defaults: ["tflite_mtk_defaults"],
    srcs: [
        "mtk_utils.cc",
    ],
}

build = [
    "tflite_mtk_ext_test.bp",
]
