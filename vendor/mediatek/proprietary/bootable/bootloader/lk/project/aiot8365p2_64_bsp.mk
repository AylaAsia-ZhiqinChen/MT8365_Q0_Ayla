#
LOCAL_DIR := $(GET_LOCAL_DIR)

TARGET := aiot8365p2_64_bsp

MODULES += app/mt_boot \
           dev/lcm

MTK_EMMC_SUPPORT = yes
DEFINES += MTK_NEW_COMBO_EMMC_SUPPORT
MTK_KERNEL_POWER_OFF_CHARGING = yes
#DEFINES += SWCHR_POWER_PATH
#DEFINES += MTK_BQ24261_SUPPORT
MTK_CHARGER_NEW_ARCH := yes
MTK_RT9466_SUPPORT := yes
MTK_CHARGER_INTERFACE := yes
MTK_LCM_PHYSICAL_ROTATION = 0
CUSTOM_LK_LCM="jd936x_wxga_dsi_vdo"
#nt35595_fhd_dsi_cmd_truly_nt50358 = yes
PRJ_SELINUX_STATUS = 3
MTK_VERIFIED_BOOT_SUPPORT = yes

BOOT_LOGO=wxga

#DEFINES += WITH_DEBUG_DCC=1
DEFINES += WITH_DEBUG_UART=1
#DEFINES += WITH_DEBUG_FBCON=1

MTK_IN_HOUSE_TEE_SUPPORT=yes
MTK_SEC_VIDEO_PATH_SUPPORT=no
MTK_WVDRM_L1_SUPPORT=no
DEFINES += KERNEL_32BIT
MTK_DM_VERITY_OFF = no
MTK_USERIMAGES_USE_F2FS = yes
MTK_MRDUMP_ENABLE := no
MTK_VPU_SUPPORT = yes
MTK_VPU_LOAD_IMAGE = yes
FASTBOOT_WHOLE_FLASH_SUPPORT = no
MTK_FBTOOL_SUPPORT = no
ifeq ($(MTK_FBTOOL_SUPPORT),yes)
MTK_SECURITY_SW_SUPPORT = no
MTK_SEC_FASTBOOT_UNLOCK_SUPPORT = no
else
MTK_SECURITY_SW_SUPPORT = yes
MTK_SEC_FASTBOOT_UNLOCK_SUPPORT = yes
endif
