package vendor.mediatek.hardware.radio@2.4;

import @2.3::IRadioResponse;
import android.hardware.radio@1.1::types;
import android.hardware.radio@1.0::types:RadioResponseInfo;

/**
 * Interface declaring response functions to solicited radio requests.
 * Response functions defined in this interface are as per following convention:
 * <xyz>Response is response to IRadio.<xyz>
 */
interface IRadioResponse extends @2.3::IRadioResponse {
    /**
     * The response of setWifiPingResultResponse
     *
     * @param info Response info struct containing response type, serial no. and error.
     */
    oneway setWifiPingResultResponse(RadioResponseInfo info);
};
