package vendor.mediatek.hardware.radio_op@1.1;

import android.hardware.radio@1.0::IRadio;
import android.hardware.radio@1.0::types;

/*
 * Interface declaring response functions to solicited radio requests.
 * Response functions defined in this interface are as per following convention:
 * <xyz>Response is response to IRadio.<xyz>
 */
interface IDigitsRadioResponse {

    /**
     * The response of setDigitsLine
     *
     * @param info Response info struct containing response type, serial no. and error.
     */
    oneway setDigitsLineResponse(RadioResponseInfo info);


    /**
     * The response of setTrn
     *
     * @param info Response info struct containing response type, serial no. and error.
     */
    oneway setTrnResponse(RadioResponseInfo info);

};
