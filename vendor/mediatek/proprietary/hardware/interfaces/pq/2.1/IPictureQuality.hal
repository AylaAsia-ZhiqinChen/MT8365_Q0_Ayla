package vendor.mediatek.hardware.pq@2.1;

import @2.0::IPictureQuality;
import @2.0::types;

/*
 * Interface declaring display quality enhancement.
 */

interface IPictureQuality extends @2.0::IPictureQuality {
    typedef vendor.mediatek.hardware.pq@2.0::Result Result;

    /**
     * Set ESS smart backlight strength.
     *
     * @param level decides ESS smart backlight strength level (Range: 0 ~ 255).
     * @return retval OK if setSmartBacklightStrength successfully executed.
     */
    setSmartBacklightStrength(int32_t level) generates (Result retval);
};
