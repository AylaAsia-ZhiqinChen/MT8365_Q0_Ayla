package vendor.mediatek.hardware.camera.callbackclient@1.1;
/*
 * This is used to CallbackClient.
 */

enum MtkCBClientID : uint32_t {
    // default
    MTK_CALLBACK_CLIENT_ID_DEFAULT     = 0x00000000,
    // callbackclient for single cam
    MTK_CALLBACK_CLIENT_ID_SINGLE      = MTK_CALLBACK_CLIENT_ID_DEFAULT,
    // callbackclient for dual cam
    MTK_CALLBACK_CLIENT_ID_DUAL        = 0x00000001
};

enum MtkCBClientCmdType : uint32_t {
    // set
    MTK_CALLBACK_CLIENT_CMD_SET_CALLBACK_ID      = 0x00000001,

    // dump callback client info
    MTK_CALLBACK_CLIENT_CMD_DUMP                 = 0x00000002
};