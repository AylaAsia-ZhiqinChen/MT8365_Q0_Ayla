
cc_binary {
    proprietary: true,
    defaults: ["hidl_defaults"],
    relative_install_path: "hw",
    name: "power_native_test_v_1_0",
    srcs: [
        "power_native_test.cpp",
    ],

    cflags: [
        "-Wall",
        "-Werror",
    ],

