// cuchar standard header
#ifndef _CUCHAR_
#define _CUCHAR_
#include <c99/yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <c99/uchar.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <c99/uchar.h>
#endif /* _STD_USING */

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD mbstate_t; using _CSTD size_t;
using _CSTD mbrtoc16; using _CSTD c16rtomb;
using _CSTD mbrtoc32; using _CSTD c32rtomb;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _CUCHAR_ */

