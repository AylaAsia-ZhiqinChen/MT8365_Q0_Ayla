system "sys"
(
  properties
  (
    type = "System";
    version = "1.0";
    variant = "";
    loadfile_extension = "";
    description = "";
    onecyclevalue = "10.0";
    onecycletime = "ns";
    simulationresolution = "1 ps";
    simulationdefaulttime = "1 ns";
  )
  parameters ()
  components
  (

    instance "core0"
    (
      model   = "xtsc_core_sd";
      version = "";
      variant = "";

      parameters
      (
        "SimTargetOutput" = "core0_output.log";
      )

    )

    instance "router"
    (
      model   = "xtsc_router_sd_2";
      version = "";
      variant = "";

      parameters
      (
        "default_port_num" = "1";
        "routing_table" = "../../xtsc_router/routing.tab";
      )

    )

    instance "sysrom"
    (
      model   = "xtsc_memory_sd_1";
      version = "";
      variant = "";

      parameters
      (
        "block_read_delay" = "1";
        "block_write_delay" = "1";
        "block_write_repeat" = "1";
        "block_write_response" = "1";
        "byte_width" = "8";
        "rcw_response" = "1";
        "read_delay" = "1";
        "write_delay" = "1";
      )

    )

    instance "sysram"
    (
      model   = "xtsc_memory_sd_1";
      version = "";
      variant = "";

      parameters
      (
        "block_read_delay" = "1";
        "block_write_delay" = "1";
        "block_write_repeat" = "1";
        "block_write_response" = "1";
        "byte_width" = "8";
        "rcw_response" = "1";
        "read_delay" = "1";
        "write_delay" = "1";
      )

    )

  )
  connections
  (
    "core0::m_pif_request_port<-->router::m_request_export" = "core0":"m_pif_request_port" to "router":"m_request_export";
    "router::m_respond_port<-->core0::m_pif_respond_export" = "router":"m_respond_port" to "core0":"m_pif_respond_export";
    "router::m_request_ports[0]<-->sysrom::m_request_exports[0]" = "router":"m_request_ports[0]" to "sysrom":"m_request_exports[0]";
    "sysrom::m_respond_ports[0]<-->router::m_respond_exports[0]" = "sysrom":"m_respond_ports[0]" to "router":"m_respond_exports[0]";
    "router::m_request_ports[1]<-->sysram::m_request_exports[0]" = "router":"m_request_ports[1]" to "sysram":"m_request_exports[0]";
    "sysram::m_respond_ports[0]<-->router::m_respond_exports[1]" = "sysram":"m_respond_ports[0]" to "router":"m_respond_exports[1]";
  )
  portmaps ()
)
