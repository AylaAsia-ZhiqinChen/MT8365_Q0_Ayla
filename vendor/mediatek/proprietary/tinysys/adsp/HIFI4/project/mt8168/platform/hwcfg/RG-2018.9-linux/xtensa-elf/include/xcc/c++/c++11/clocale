// clocale standard header
#ifndef _CLOCALE_
#define _CLOCALE_

 #if _IS_GHS

 #if defined(__NAMESPACES) && !defined(__EMBEDDED_CXX_HEADERS)
namespace std {
    using ::lconv;
    using ::localeconv;
    using ::setlocale;
}
 #endif /* defined(__NAMESPACES), etc. */

 #else /* _IS_GHS */
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <locale.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <locale.h>
#endif /* _STD_USING */

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD lconv; using _CSTD localeconv; using _CSTD setlocale;
_STD_END
 #endif /* _GLOBAL_USING */

 #endif /* _IS_GHS */
#endif /* _CLOCALE_ */

