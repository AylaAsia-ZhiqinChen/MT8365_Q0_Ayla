// cstdarg standard header
#ifndef _CSTDARG_
#define _CSTDARG_
#include <yvals.h>

 #if defined(__MWERKS__)	/* compiler test */
  #include <va_list.h>
  #include <stdarg.win32.h>

 #else /* __MWERKS__ */
  #include <stdarg.h>
 #endif /* __MWERKS__ */

 #ifdef _GLOBAL_USING
_STD_BEGIN
using ::va_list;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _CSTDARG_ */

