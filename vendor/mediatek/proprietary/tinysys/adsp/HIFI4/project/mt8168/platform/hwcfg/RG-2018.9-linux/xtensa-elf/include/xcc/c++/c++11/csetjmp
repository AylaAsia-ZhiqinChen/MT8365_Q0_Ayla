// csetjmp standard header
#ifndef _CSETJMP_
#define _CSETJMP_

 #if _IS_GHS
#include <setjmp.h>

 #if defined(__NAMESPACES) && !defined(__EMBEDDED_CXX_HEADERS)
namespace std {
using ::jmp_buf; using ::longjmp;

  #ifndef setjmp
using ::setjmp;
  #endif /* setjmp */
}	// namespace std
 #endif /* defined(__NAMESPACES), etc. */

 #else /* _IS_GHS */
#include <yvals.h>
#include <setjmp.h>

 #ifdef _GLOBAL_USING
_STD_BEGIN
using ::jmp_buf; using ::longjmp;

  #ifndef setjmp
using ::setjmp;
  #endif /* setjmp */
_STD_END
 #endif /* _GLOBAL_USING */

 #endif /* _IS_GHS */

 #ifndef setjmp
  #define setjmp	setjmp
 #endif /* setjmp */

#endif /* _CSETJMP_ */

