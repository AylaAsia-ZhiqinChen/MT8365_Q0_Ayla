state STATEOUT 32 32'b0 add_read_write export

import_wire WIREIN 32

operation ReadWire {out AR v} {in WIREIN}
{
    assign v = WIREIN;
}

queue Q_OUT 32 out
queue Q_IN  32 in

operation PushQ {in AR a} {out Q_OUT}
{
   assign Q_OUT = a;
}

operation PopQ {out AR a} {in Q_IN}
{
   assign a = Q_IN;
}

