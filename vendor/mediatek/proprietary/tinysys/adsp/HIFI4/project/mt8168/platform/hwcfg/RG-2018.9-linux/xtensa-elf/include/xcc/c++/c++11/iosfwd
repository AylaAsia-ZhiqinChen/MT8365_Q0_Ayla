using _STD istream;
using _STD ostream;
using _STD iostream;

using _STD filebuf;
using _STD ifstream;
using _STD ofstream;
using _STD fstream;

using _STD stdiobuf;
using _STD stdiostream;
 #endif /* _HAS_NAMESPACE */

 #endif /* _HAS_TRADITIONAL_IOSTREAMS */
#endif /* _IOSFWD_ */

