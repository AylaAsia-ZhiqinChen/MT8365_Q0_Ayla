<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_above="@+id/bottom_button_container">
        

                <LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal"
					android:gravity="center" >

					<TextView
						android:id="@+id/psam_chip_test_title"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:textSize="@dimen/test_item_text_size"
						/>

					<TextView
						android:id="@+id/psam_chip_test_result"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:textSize="@dimen/test_item_text_size"
						/>

				</LinearLayout>
				
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal"
					android:gravity="center" >

					<TextView
						android:id="@+id/psam_card_test_title"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:textSize="@dimen/test_item_text_size"
						/>

					<TextView
						android:id="@+id/psam_card_test_result"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:textSize="@dimen/test_item_text_size"
						/>

				</LinearLayout>

    </LinearLayout>
    
    <include layout="@layout/bottom_button"/>

</RelativeLayout>
