	gsensor_icm426@6a{
		compatible = "mediatek,gsensor_icm426";
		reg = <0x6a>;
		status = "okay";
	};	
	gsensor_bmi160@68{
		compatible = "mediatek,gsensor_bmi160";
		reg = <0x68>;
		status = "okay";
	};
	gyro_icm426@6c {
